pipeline {
    agent any

    stages {
        stage ("checkout scm") {
            steps {
                git "https://github.com/tejaswi-teju/myproject.git"
            }
        }

        stage ("Build Dockerfile") {
            steps {
                sh "docker build -t myapp:latest ./terraform.tf"
            }
        }

        /*stage ("Push Image to AWS ECR") {
            steps {
                sh ""
            }
        }*/
    }
}